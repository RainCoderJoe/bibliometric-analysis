@article{ ISI:000335891700007,
Author = {Kogut, Bruce and Colomer, Jordi and Belinky, Mariano},
Title = {{Structural equality at the top of the corporation: Mandated quotas for
   women directors}},
Journal = {{STRATEGIC MANAGEMENT JOURNAL}},
Year = {{2014}},
Volume = {{35}},
Number = {{6}},
Pages = {{891-902}},
Month = {{JUN}},
Abstract = {{We propose a concept of structural equality as a compromise between
   competing policy preferences of equality and individual liberty to
   address a stunning property of the governance of corporations, namely,
   the paucity of female directors on corporate boards. An argument for
   imposing a quota for women directors on boards is the need to disrupt
   structural impediments to permit endogenous mechanisms to sustain female
   recruitment beyond a critical mass. Using estimates from the Norwegian
   experiment, we apply an agent-based model to American board data to show
   that modest numerical quotas generate well-connected networks of women
   directors who attain equality in their centrality and influence. The
   analysis demonstrates the utility of computational social science for
   identifying policies that generate alternative and possible worlds of
   greater structural equality. Copyright (c) 2013 John Wiley \& Sons, Ltd.}},
