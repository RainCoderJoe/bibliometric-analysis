@article{ ISI:000432098300019,
Author = {Vargo, Chris J. and Guo, Lei and Amazeen, Michelle A.},
Title = {{The agenda-setting power of fake news: A big data analysis of the online
   media landscape from 2014 to 2016}},
Journal = {{NEW MEDIA \& SOCIETY}},
Year = {{2018}},
Volume = {{20}},
Number = {{5}},
Pages = {{2028-2049}},
Month = {{MAY}},
Abstract = {{This study examines the agenda-setting power of fake news and
   fact-checkers who fight them through a computational look at the online
   mediascape from 2014 to 2016. Although our study confirms that content
   from fake news websites is increasing, these sites do not exert
   excessive power. Instead, fake news has an intricately entwined
   relationship with online partisan media, both responding and setting its
   issue agenda. In 2016, partisan media appeared to be especially
   susceptible to the agendas of fake news, perhaps due to the election.
   Emerging news media are also responsive to the agendas of fake news, but
   to a lesser degree. Fake news coverage itself is diverging and becoming
   more autonomous topically. While fact-checkers are autonomous in their
   selection of issues to cover, they were not influential in determining
   the agenda of news media overall, and their influence appears to be
   declining, illustrating the difficulties fact-checkers face in
   disseminating their corrections.}},
