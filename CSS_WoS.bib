@article{ ISI:000260249800002,
Author = {Squazzoni, Flaminio},
Title = {{A (computational) social science perspective on societal transitions}},
Journal = {{COMPUTATIONAL AND MATHEMATICAL ORGANIZATION THEORY}},
Year = {{2008}},
Volume = {{14}},
Number = {{4}},
Pages = {{266-282}},
