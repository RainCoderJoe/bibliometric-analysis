@article{ ISI:000344971100012,
Author = {Gaskin, James and Berente, Nicholas and Lyytinen, Kalle and Yoo,
   Youngjin},
Title = {{TOWARD GENERALIZABLE SOCIOMATERIAL INQUIRY: A COMPUTATIONAL APPROACH FOR
   ZOOMING IN AND OUT OF SOCIOMATERIAL ROUTINES}},
